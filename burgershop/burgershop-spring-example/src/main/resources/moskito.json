  "@errorHandlingConfig": {
    "autoChartErrors": true,
    "autoChartErrorsInterval": "1m",
    "logErrors": true,
    "countRethrows": true,
    "@catchers": [
      {
        "target": "LOGANDMEMORY",
        "clazz":  "java.lang.IllegalArgumentException",
        "parameter": "IllegalArgExcLog"

      },
      {
        "target": "MEMORY",
        "clazz":  "*",
        "parameter": ""

      }

    ]
  },

