                "@guards": [
                    {"value": "10", "direction": "DOWN", "status": "GREEN"},
                    {"value": "10", "direction": "UP", "status": "YELLOW"},
                    {"value": "15", "direction": "UP", "status": "ORANGE"},
                    {"value": "20", "direction": "UP", "status": "RED"},
                    {"value": "50", "direction": "UP", "status": "PURPLE"}
